{
  "name": "@manishbasnetnp/shared-common",
  "version": "2.0.0",
  "description": "A common functionalities shared between different services specifically used for Node.js.",
  "keywords": [
    "shared-common"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Manish Basnet",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/manishbasnet/shared-common"
  },
  "scripts": {
    "start": "npm run test",
    "watch:test": "npm t -- --watch",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "publish": "git push --tags && npm publish --access=public"
  },
  "engines": {
    "node": ">= 10"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.11.6",
    "json-loader": "0.5.4",
    "npm-run-all": "2.3.0",
    "rimraf": "2.5.4",
    "webpack": "1.13.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
